
# Kconfig file for package strings
menuconfig PKG_USING_POSIX_STRINGS
    bool "Extension POSIX string.h functions (4.3BSD, POSIX.1-2001)"
    default n

if PKG_USING_POSIX_STRINGS

    config PKG_POSIX_STRINGS_PATH
        string
        default "/packages/system/POSIX/strings"

    config PKG_POSIX_STRINGS_VER
       string
       default "latest"

endif

